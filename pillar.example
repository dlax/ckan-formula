ckan:
  lookup:
    ckan_home: /home/ckan
    src_dir: /home/ckan/src
    standard_plugins:
      - multilingual_dataset
      - multilingual_group
      - multilingual_tag
    install_extensions:
      - qa
      - report
      - archiver
      - harvest
      - dcat
      - spatial
    extensions:
      archiver:
        options:
          ckanext-archiver.archive_dir: /var/www/ckan-resources
          ckanext-archiver.cache_url_root: http://localhost:9876/resources
      dcat:
        plugins:
          - dcat
          - dcat_rdf_harvester
