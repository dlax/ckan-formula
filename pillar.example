ckan:
  lookup:
    ckan_home: /home/ckan
    src_dir: /home/ckan/src
    extensions:
      datastorer:
        set_logger: true
      qa:
        rev: stable
        plugins:
          - qa
      harvest:
        plugins:
          - harvest
        options:
          mq.type: redis
      dcat:
        repourl: https://github.com/ckan/ckanext-dcat
        plugins:
          - dcat
          - dcat_rdf_harvester
      spatial:
        plugins:
          - spatial_metadata
          - spatial_query
